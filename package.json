{
  "name": "@vilaboim/moo-cli",
  "version": "0.6.1",
  "description": "A MOOvelous CLI",
  "main": "dist/index.js",
  "bin": {
    "moo-cli": "dist/index.js",
    "moo": "dist/index.js"
  },
  "scripts": {
    "watch": "tsc -w",
    "check": "tsc --noEmit",
    "lint": "tslint -p . -c tslint.json --fix",
    "build": "npm run check && npm run lint && tsc",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vilaboim/moo-cli.git"
  },
  "keywords": [
    "cli",
    "command",
    "line",
    "interface",
    "tool",
    "frontend",
    "web",
    "projects",
    "js"
  ],
  "author": "Alex Bruno <alexcaceres@softbox.com.br>, Lucas Vilaboim <lucasvilaboim@softbox.com.br>, Pablo Henrique Penha Silva <pablohpsilva@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vilaboim/moo-cli/issues"
  },
  "homepage": "https://github.com/vilaboim/moo-cli#readme",
  "lint-staged": {
    "*.{ts}": [
      "npm run lint -- --fix",
      "git add"
    ],
    "packages/**/bin/*": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "dependencies": {
    "commander": "^2.16.0",
    "cowsay": "^1.3.1",
    "inquirer": "^6.0.0",
    "motivation": "^1.0.3",
    "mustache": "^2.3.0",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "@semantic-release/npm": "^5.2.0",
    "@types/inquirer": "0.0.43",
    "@types/mustache": "^0.8.31",
    "@types/node": "^10.5.3",
    "@types/shelljs": "^0.8.0",
    "chalk": "^2.4.1",
    "conventional-changelog": "^2.0.3",
    "execa": "^1.0.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "semantic-release": "^15.13.27",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "plugins": [
    [
      "@semantic-release/npm",
      {
        "npmPublish": false
      }
    ]
  ]
}
